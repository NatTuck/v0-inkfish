
<h1>Show Course</h1>

<p>
  <span><%= link "Edit", to: Routes.course_path(@conn, :edit, @course) %></span> |
  <span><%= link "Back", to: Routes.course_path(@conn, :index) %></span>
</p>

<ul>
  <li>
    <strong>Name:</strong>
    <%= @course.name %>
  </li>
  <li>
    <strong>Start date:</strong>
    <%= @course.start_date %>
  </li>
  <li>
    <strong>Footer:</strong>
    <%= @course.footer %>
  </li>
</ul>

